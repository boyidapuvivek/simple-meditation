{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "constants/affirmation-images.ts",
    "constants/meditation-images.ts",
    "declarations.d.ts",
    "src/**/*"]
}
